{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/$",
      "dest": "/api/index"
    },
    {
      "src": "^/gantt-demo/?$",
      "dest": "/api/index"
    },
    {
      "src": "^/gantt-demo(/[^./]+)?$",
      "dest": "/api/index"
    },
    {
      "handle": "filesystem"
    }
  ]
}